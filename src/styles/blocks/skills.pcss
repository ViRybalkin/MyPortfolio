.skills{

  &__wrapper{
    display: flex;
    justify-content: flex-end;
  }
  &__item{
    margin-bottom: 54px;
    &:last-child{
      margin-bottom: 0;
    }
  }
  &-row{
    &__list{
      display: flex;
    }
    &__item{
      margin-right: 66px;
      &:last-child{
        margin-right: 0;
      }
    }
    &__title{
      margin-bottom: 40px;
    }
  }
}