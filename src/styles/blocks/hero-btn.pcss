.hero-btn{
  display: flex;
  align-items: center;
  justify-content: center;
  &__button{
    font-weight: 600;
    display: flex;
    &:after{
     margin-left: 15px;
     content: "";
     display: block;
     background: svg-load('scroll.svg', fill=$dark, width=100%, height=100%);
     width: 25px;
     height: 25px;
   }
  }
}